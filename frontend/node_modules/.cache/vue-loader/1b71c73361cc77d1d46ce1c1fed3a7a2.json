 justify-content-between align-items-center\">\r\n                      <!-- <div class=\"btn-group\">\r\n                      <a href=\"\" class=\"btn btn-sm btn-outline-primary\" role=\"button\" aria-pressed=\"true\">View</a>\r\n                      <a href=\"\" class=\"btn btn-sm btn-outline-secondary\" role=\"button\" aria-pressed=\"true\">Edit</a>\r\n                      </div>\r\n                      <small class=\"text-muted\">9 mins</small> -->\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import Navbar from '../components/Navbar'\r\n  import { getAPI } from '../axios-api'\r\n  import { mapStore } from 'vuex'\r\n\r\n  export default {\r\n    name: 'Products',\r\n\r\n    data () {\r\n      return {\r\n          APIData: []\r\n        }\r\n    },\r\n  \r\n    components: {\r\n      Navbar\r\n    },\r\n\r\n    computed: mapStore(['APIData']),\r\n\r\n    created () {\r\n      getAPI.get('/api/product/',)\r\n      .then(response => {\r\n        console.log('Post API has recieved data')\r\n        this.APIData = response.data\r\n        console.log(this.APIData)\r\n      })\r\n      .catch(err => {\r\n        console.log(err)\r\n      })\r\n    }\r\n\r\n    // created () {\r\n    //   getAPI.get('/posts/', { headers: { Authorization: `Bearer ${this.$store.state.accessToken}` } })\r\n    //   .then(response => {\r\n    //     this.$store.state.APIData = response.data\r\n    //   })\r\n    //   .catch(err => {\r\n    //     console.log(err)\r\n    //   })\r\n    // }\r\n  \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n\r\n"]}]}